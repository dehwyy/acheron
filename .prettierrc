{
	"tabWidth": 2,
	"printWidth": 140,
	"semi": false,
	"useTabs": false,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"singleAttributePerLine": false,
	"arrowParens": "always",
	"trailingComma": "none",
	"quoteProps": "as-needed",
	"endOfLine": "lf",
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"^(react|next(.*))$",
		"<THIRD_PARTY_MODULES>",
		"<TYPES>",
		"",
		"^@/(.*)$",
		"^~/(.*)$",
		"^[./]",
		"<TYPES>^[.]",
		"^(.*)(sass|css|scss)$"
	],
	"importOrderParserPlugins": [
		"classProperties",
		"decorators-legacy",
		"jsx",
		"typescript"
	],
	"overrides": []
}
